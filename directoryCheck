import os, sys, subprocess

def find_files(path):
    result = []
    direct = []
    fil = []
    for root, dirs, files in os.walk(path):
        if dirs not in direct:
            direct.append(dirs)
        if files not in fil:
            fil.append(files)

    print direct
    print fil

    return

